# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lsd='ls -d */'

alias vim=nvim
alias omniview="google-chrome http://localhost:9000/"
export VISUAL=vim
export EDITOR="$VISUAL"
#alias build="catkin build walker_planner -j8"

# No need to type cd
setopt AUTO_CD
# Use `~` before the name
