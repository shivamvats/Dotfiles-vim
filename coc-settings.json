{
    "python.formatting.provider" : "black",
    "python.jediEnabled": "true",

    "suggest.maxPreviewWidth": "45",
    "suggest.detailMaxLength": "30",
    "suggest.labelMaxLength": "15",

    "list.source.grep.command": "ag",
    "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "expr:GetDeleteAction"
    },
    "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
    },
}
